_G.Auto_Fram = false
_G.Instant_Kill = false

_G.MonsterSelect = "Lower Moon Demon 4"

_G.Tool = "Combat"

_G.Mode = "Above" -- Above, Behide, Under
_G.Distance = 5


while _G.Auto_Fram == true do task.wait()
	pcall(function()
		if game:GetService("Players").LocalPlayer.Quest.Num.Value ~= 10 and game:GetService("Players").LocalPlayer.Quest.Num.Value ~= 0 then
			game:GetService("Players").LocalPlayer.PlayerGui.QuestBar.ingame.quest["1"].RemoteEvent:FireServer()
		elseif game:GetService("Players").LocalPlayer.Quest.Num.Value == 10 then
			for i, v in pairs(game:GetService("Workspace").CharactersAndNPCs:GetChildren()) do
				if not game:GetService("Players"):FindFirstChild(v.Name) and v.Name == _G.MonsterSelect and v.Humanoid.Health > 0 then
					repeat task.wait()
						if _G.Mode == "Above" then
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance, 0) * CFrame.fromOrientation(300, 0 ,0)
						elseif _G.Mode == "Behind" then
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, (_G.Distance - _G.Distance * 2))
						elseif _G.Mode == "Under" then
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, (_G.Distance - _G.Distance * 2), 0) * CFrame.fromOrientation(900, 0, 0)
						end


						setfflag("HumanoidParallelRemoveNoPhysics", "False")
						setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
						game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)


						if not game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.Tool) then
							game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack[_G.Tool])
						else	
							game:GetService("ReplicatedStorage").Remotes.Attack:FireServer("M1", _G.Tool, false)
						end
					until _G.Auto_Fram == false or v.Humanoid.Health == 0 or game:GetService("Players").LocalPlayer.Character.Humanoid.Health == 0
				end
			end
		elseif game:GetService("Players").LocalPlayer.Quest.Num.Value == 0 then
			game:GetService("ReplicatedStorage").Events.QuestTake:FireServer("10","1","??? YEN, ??? XP","Nerd Ware","It Real Lower 4?")
		end
	end)
end


while _G.Instant_Kill == true do task.wait()
	pcall(function()
		for i, v in pairs(game:GetService("Workspace").CharactersAndNPCs:GetChildren()) do
			if v.ClassName == "Model" and v:FindFirstChild("Killers") and v.Killers:FindFirstChild(game:GetService("Players").LocalPlayer.Name) and (game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 15 then
				v.Humanoid.Health = 0
			end
		end
	end)
end
